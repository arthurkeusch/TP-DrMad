   },\n    itemButtonClick(index) {\n      this.$emit('item-button-clicked', {\n        index,\n        amount: this.itemAmount ? this.amountInput[index] : undefined,\n      });\n    },\n    listButtonClick() {\n      const selectedItems = this.checked.reduce((acc, isSelected, index) => {\n        if (isSelected) {\n          acc.push({\n            index,\n            amount: this.itemAmount ? this.amountInput[index] : undefined,\n          });\n          // Désélectionner l'item\n          this.$set(this.checked, index, false);\n        }\n        return acc;\n      }, []);\n      this.$emit('list-button-clicked', selectedItems);\n    },\n    displayItem(item) {\n      return this.fields.map((field) => item[field]).join(' ');\n    },\n    updateAmount(index) {\n      // Mettre à jour la valeur du champ numérique dans le tableau\n      this.$emit('amount-updated', {\n        index,\n        amount: this.amountInput[index],\n      });\n    },\n  },\n};\n</script>"],"mappings":";AA6BA;EACAA,IAAA;EACAC,KAAA;IACAC,IAAA,EAAAC,KAAA;IACAC,MAAA,EAAAD,KAAA;IACAE,SAAA,EAAAC,OAAA;IACAC,OAAA,EAAAJ,KAAA;IACAK,UAAA,EAAAC,MAAA;IACAC,UAAA,EAAAD,MAAA;IACAE,UAAA,EAAAL;EACA;EACAJ,KAAA;IACA;MACAU,WAAA,OAAAV,IAAA,GAAAC,KAAA,MAAAD,IAAA,CAAAW,MAAA,EAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAAC,KAAA;MACA,MAAAC,YAAA,QAAAX,OAAA,CAAAY,OAAA,CAAAF,KAAA;MAEA,IAAAC,YAAA;QACA;QACA;QACA,KAAAX,OAAA,CAAAa,IAAA,CAAAH,KAAA;MACA;QACA;QACA;QACA,KAAAV,OAAA,CAAAc,MAAA,CAAAH,YAAA;MACA;IACA;IACAI,gBAAAL,KAAA;MACA,KAAAM,KAAA;QACAN,KAAA;QACAO,MAAA,OAAAb,UAAA,QAAAC,WAAA,CAAAK,KAAA,IAAAQ;MACA;IACA;IACAC,gBAAA;MACA,MAAAC,aAAA,QAAApB,OAAA,CAAAqB,MAAA,EAAAC,GAAA,EAAAC,UAAA,EAAAb,KAAA;QACA,IAAAa,UAAA;UACAD,GAAA,CAAAT,IAAA;YACAH,KAAA;YACAO,MAAA,OAAAb,UAAA,QAAAC,WAAA,CAAAK,KAAA,IAAAQ;UACA;UACA;UACA,KAAAM,IAAA,MAAAxB,OAAA,EAAAU,KAAA;QACA;QACA,OAAAY,GAAA;MACA;MACA,KAAAN,KAAA,wBAAAI,aAAA;IACA;IACAK,YAAAC,IAAA;MACA,YAAA7B,MAAA,CAAA8B,GAAA,CAAAC,KAAA,IAAAF,IAAA,CAAAE,KAAA,GAAAC,IAAA;IACA;IACAC,aAAApB,KAAA;MACA;MACA,KAAAM,KAA