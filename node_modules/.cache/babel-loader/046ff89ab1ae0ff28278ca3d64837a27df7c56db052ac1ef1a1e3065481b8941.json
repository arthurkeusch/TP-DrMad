{"ast":null,"code":"import CheckedList from './CheckedList.vue'; // Assurez-vous d'ajuster le chemin selon votre structure de projet\nexport default {\n  components: {\n    CheckedList\n  },\n  data() {\n    return {\n      viruses: [/* Votre liste de virus */],\n      checkedViruses: Array(this.viruses.length).fill(false)\n    };\n  },\n  methods: {\n    toggleVirusCheckbox(index) {\n      this.$set(this.checkedViruses, index, !this.checkedViruses[index]);\n    },\n    addToCart({\n      index,\n      amount\n    }) {\n      // Logique pour ajouter un virus au panier de l'utilisateur courant (dans shopUser)\n      const selectedVirus = this.viruses[index];\n      // Ajouter la logique pour mettre à jour le panier dans shopUser avec le virus sélectionné et la quantité\n      console.log(`Ajouter au panier : ${selectedVirus.name}, Quantité : ${amount}`);\n    },\n    addAllToCart(selectedItems) {\n      // Logique pour ajouter tous les virus sélectionnés au panier de l'utilisateur courant (dans shopUser)\n      selectedItems.forEach(({\n        index,\n        amount\n      }) => {\n        const selectedVirus = this.viruses[index];\n        // Ajouter la logique pour mettre à jour le panier dans shopUser avec le virus sélectionné et la quantité\n        console.log(`Ajouter au panier : ${selectedVirus.name}, Quantité : ${amount}`);\n      });\n    },\n    updateAmount({\n      index,\n      amount\n    }) {\n      // Logique pour mettre à jour la quantité d'un virus dans la liste\n      console.log(`Mise à jour de la quantité de ${this.viruses[index].name} : ${amount}`);\n    }\n  }\n};","map":{"version":3,"names":["CheckedList","components","data","viruses","checkedViruses","Array","length","fill","methods","toggleVirusCheckbox","index","$set","addToCart","amount","selectedVirus","console","log","name","addAllToCart","selectedItems","forEach","updateAmount"],"sources":["src/components/ItemsList.vue"],"sourcesContent":["<!-- ItemsList.vue -->\r\n<template>\r\n    <div>\r\n        <h2>Liste des articles</h2>\r\n        <CheckedList\r\n                :data=\"viruses\"\r\n                :fields=\"['name', 'price', 'promotions']\"\r\n                :itemCheck=\"true\"\r\n                :checked=\"checkedViruses\"\r\n                :itemButton=\"{ show: true, text: 'Ajouter au panier' }\"\r\n                :listButton=\"{ show: true, text: 'Ajouter au panier' }\"\r\n                :itemAmount=\"true\"\r\n                @checked-changed=\"toggleVirusCheckbox\"\r\n                @item-button-clicked=\"addToCart\"\r\n                @list-button-clicked=\"addAllToCart\"\r\n                @amount-updated=\"updateAmount\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport CheckedList from './CheckedList.vue'; // Assurez-vous d'ajuster le chemin selon votre structure de projet\r\nexport default {\r\n    components: {\r\n        CheckedList,\r\n    },\r\n    data() {\r\n        return {\r\n            viruses: [/* Votre liste de virus */],\r\n            checkedViruses: Array(this.viruses.length).fill(false),\r\n        };\r\n    },\r\n    methods: {\r\n        toggleVirusCheckbox(index) {\r\n            this.$set(this.checkedViruses, index, !this.checkedViruses[index]);\r\n        },\r\n        addToCart({ index, amount }) {\r\n            // Logique pour ajouter un virus au panier de l'utilisateur courant (dans shopUser)\r\n            const selectedVirus = this.viruses[index];\r\n            // Ajouter la logique pour mettre à jour le panier dans shopUser avec le virus sélectionné et la quantité\r\n            console.log(`Ajouter au panier : ${selectedVirus.name}, Quantité : ${amount}`);\r\n        },\r\n        addAllToCart(selectedItems) {\r\n            // Logique pour ajouter tous les virus sélectionnés au panier de l'utilisateur courant (dans shopUser)\r\n            selectedItems.forEach(({ index, amount }) => {\r\n                const selectedVirus = this.viruses[index];\r\n                // Ajouter la logique pour mettre à jour le panier dans shopUser avec le virus sélectionné et la quantité\r\n                console.log(`Ajouter au panier : ${selectedVirus.name}, Quantité : ${amount}`);\r\n            });\r\n        },\r\n        updateAmount({ index, amount }) {\r\n            // Logique pour mettre à jour la quantité d'un virus dans la liste\r\n            console.log(`Mise à jour de la quantité de ${this.viruses[index].name} : ${amount}`);\r\n        },\r\n    },\r\n};\r\n</script>"],"mappings":"AAqBA,OAAAA,WAAA;AACA;EACAC,UAAA;IACAD;EACA;EACAE,KAAA;IACA;MACAC,OAAA;MACAC,cAAA,EAAAC,KAAA,MAAAF,OAAA,CAAAG,MAAA,EAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,oBAAAC,KAAA;MACA,KAAAC,IAAA,MAAAP,cAAA,EAAAM,KAAA,QAAAN,cAAA,CAAAM,KAAA;IACA;IACAE,UAAA;MAAAF,KAAA;MAAAG;IAAA;MACA;MACA,MAAAC,aAAA,QAAAX,OAAA,CAAAO,KAAA;MACA;MACAK,OAAA,CAAAC,GAAA,wBAAAF,aAAA,CAAAG,IAAA,gBAAAJ,MAAA;IACA;IACAK,aAAAC,aAAA;MACA;MACAA,aAAA,CAAAC,OAAA;QAAAV,KAAA;QAAAG;MAAA;QACA,MAAAC,aAAA,QAAAX,OAAA,CAAAO,KAAA;QACA;QACAK,OAAA,CAAAC,GAAA,wBAAAF,aAAA,CAAAG,IAAA,gBAAAJ,MAAA;MACA;IACA;IACAQ,aAAA;MAAAX,KAAA;MAAAG;IAAA;MACA;MACAE,OAAA,CAAAC,GAAA,uCAAAb,OAAA,CAAAO,KAAA,EAAAO,IAAA,MAAAJ,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}